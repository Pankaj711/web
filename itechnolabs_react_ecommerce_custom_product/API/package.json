  "main": "main.js",
  "scripts": {
    "serve-fs": "export NODE_ENV=staging && pm2 start dist/main.js --name=vox_neol",
    "serve-rs": "pm2 restart vox_neol",
    "serve-ns": "pm2 delete vox_neol && npm run serve-fs",
    "start": "nodemon --exec babel-node -- main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && babel . -d dist --ignore node_modules,web && cp package.json dist",
    "serve": "node dist/main.js",
    "ri": "rm -rf node_modules && npm install",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "rp": "prettier --write \"**/*.json\" \"**/*.js\" \"!web/**/*.js\" \"!web/**/*.json\"  \"!dist/**/*.js\"",
    "precommit": "npm run rp && lint-staged"
  },
